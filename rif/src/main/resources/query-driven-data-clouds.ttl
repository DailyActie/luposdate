@prefix : <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix sf: <http://www.opengis.net/ont/sf#> .
@prefix vo: <http://www.auto-nomos.de/ontologies/vanet-ontology#> .
@prefix dce: <http://purl.org/dc/elements/1.1/> .
@prefix exp: <http://www.auto-nomos.de/ontologies/query-driven-data-clouds-examples#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix hdc: <http://www.auto-nomos.de/ontologies/query-driven-data-clouds#> .
@prefix muo: <http://purl.oclc.org/NET/muo/muo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.auto-nomos.de/ontologies/query-driven-data-clouds#> .

<http://www.auto-nomos.de/ontologies/query-driven-data-clouds#> rdf:type owl:Ontology ;
                                                                
                                                                rdfs:comment "Ontology to express the semantics of terms for HoveringDataClouds as defined in AutoNomos project and refined afterwards "^^xsd:string ;
                                                                
                                                                dce:creator "Sebastian Ebers"^^xsd:string .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://purl.org/dc/elements/1.1/creator

dce:creator rdf:type owl:AnnotationProperty .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds-examples#VelocityInKmH

exp:VelocityInKmH rdf:type owl:AnnotationProperty .





#################################################################
#
#    Datatypes
#
#################################################################


###  http://www.opengis.net/ont/geosparql#wktLiteral

geo:wktLiteral rdf:type rdfs:Datatype .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#hasDisseminationArea

hdc:hasDisseminationArea rdf:type owl:ObjectProperty ;
                         
                         rdfs:domain hdc:DataUnit ;
                         
                         rdfs:range hdc:DisseminationArea ;
                         
                         owl:inverseOf hdc:isDisseminationAreaOf .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#hasExpansion

hdc:hasExpansion rdf:type owl:ObjectProperty ;
                 
                 rdfs:domain hdc:DataCloud ;
                 
                 rdfs:range vo:Area .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#hasProductionArea

hdc:hasProductionArea rdf:type owl:ObjectProperty ;
                      
                      rdfs:domain hdc:DataCloud ;
                      
                      rdfs:range vo:Area .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#isDisseminationAreaOf

hdc:isDisseminationAreaOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#isExpansionOf

hdc:isExpansionOf rdf:type owl:ObjectProperty ;
                  
                  owl:inverseOf hdc:hasExpansion .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#isMinimumSpeedOf

hdc:isMinimumSpeedOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#isProductionAreaOf

hdc:isProductionAreaOf rdf:type owl:ObjectProperty ;
                       
                       owl:inverseOf hdc:hasProductionArea .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasBeginning

vo:hasBeginning rdf:type owl:ObjectProperty ;
                
                rdfs:range vo:Spot ;
                
                rdfs:domain vo:Topic .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasCurrentVelocity

vo:hasCurrentVelocity rdf:type owl:ObjectProperty ;
                      
                      rdfs:domain vo:MobileNode ;
                      
                      owl:inverseOf vo:isCurrentVelocityOf .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasDestination

vo:hasDestination rdf:type owl:ObjectProperty ;
                  
                  rdfs:range vo:Destination ;
                  
                  owl:inverseOf vo:isDestinationOf .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasEnd

vo:hasEnd rdf:type owl:ObjectProperty ;
          
          rdfs:range vo:Spot ;
          
          rdfs:domain vo:Topic ;
          
          owl:inverseOf vo:isEndOf .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasLifetime

vo:hasLifetime rdf:type owl:ObjectProperty ;
               
               rdfs:domain hdc:Data ;
               
               rdfs:range vo:Time ;
               
               owl:inverseOf vo:isLifetimeOf .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasLocation

vo:hasLocation rdf:type owl:ObjectProperty ;
               
               rdfs:range vo:Location ;
               
               owl:inverseOf vo:isLocationOf ;
               
               rdfs:domain geo:Feature ;
               
               owl:equivalentProperty geo:hasGeometry ;
               
               rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasMaxVelocity

vo:hasMaxVelocity rdf:type owl:ObjectProperty ;
                  
                  rdfs:range vo:AbsoluteVelocity ;
                  
                  owl:inverseOf <http://www.auto-nomos.de/ontologies/vante-ontology#isMaxVelocityOf> .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasMinimumSpeed

vo:hasMinimumSpeed rdf:type owl:ObjectProperty ;
                   
                   owl:inverseOf hdc:isMinimumSpeedOf .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasOrigin

vo:hasOrigin rdf:type owl:ObjectProperty ;
             
             rdfs:range vo:Origin ;
             
             owl:inverseOf vo:isOriginOf .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasProducer

vo:hasProducer rdf:type owl:ObjectProperty ;
               
               rdfs:domain hdc:Data ;
               
               rdfs:range vo:Node ;
               
               owl:inverseOf vo:isProducerOf .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasSensor

vo:hasSensor rdf:type owl:ObjectProperty ;
             
             rdfs:domain vo:Node ;
             
             rdfs:range vo:Sensor .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasSpeedLimit

vo:hasSpeedLimit rdf:type owl:ObjectProperty ;
                 
                 rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasTopic

vo:hasTopic rdf:type owl:ObjectProperty ;
            
            rdfs:domain hdc:Data ;
            
            rdfs:range vo:Topic .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#hasVelocity

vo:hasVelocity rdf:type owl:ObjectProperty ;
               
               rdfs:domain vo:MobileNode ;
               
               rdfs:range vo:Velocity ;
               
               owl:inverseOf vo:isVelocityOf .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isBeginningOf

vo:isBeginningOf rdf:type owl:ObjectProperty ;
                 
                 owl:inverseOf vo:hasBeginning .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isCurrentVelocityOf

vo:isCurrentVelocityOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isDestinationOf

vo:isDestinationOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isEndOf

vo:isEndOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isLifetimeOf

vo:isLifetimeOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isLocationOf

vo:isLocationOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isOriginOf

vo:isOriginOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isOvertakenBy

vo:isOvertakenBy rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isProducerOf

vo:isProducerOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isSensorOf

vo:isSensorOf rdf:type owl:ObjectProperty ;
              
              owl:inverseOf vo:hasSensor .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isSpeedLimitOf

vo:isSpeedLimitOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isTopicOf

vo:isTopicOf rdf:type owl:ObjectProperty ;
             
             owl:inverseOf vo:hasTopic .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#isVelocityOf

vo:isVelocityOf rdf:type owl:ObjectProperty .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#overtakes

vo:overtakes rdf:type owl:ObjectProperty ;
             
             owl:inverseOf vo:isOvertakenBy .



###  http://www.auto-nomos.de/ontologies/vante-ontology#isMaxVelocityOf

<http://www.auto-nomos.de/ontologies/vante-ontology#isMaxVelocityOf> rdf:type owl:ObjectProperty .



###  http://www.opengis.net/ont/geosparql#hasGeometry

geo:hasGeometry rdf:type owl:ObjectProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#creationTime

hdc:creationTime rdf:type owl:DatatypeProperty ;
                 
                 rdfs:comment "Indicates how many milliseconds have passed between January 1, 1970, 00:00:00 GMT and the creation of an individual."@en ;
                 
                 rdfs:range xsd:long .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#lastUpdate

hdc:lastUpdate rdf:type owl:DatatypeProperty ;
               
               rdfs:comment "Indicates how many milliseconds have passed between January 1, 1970, 00:00:00 GMT and the last update of an individual."@en ;
               
               rdfs:range xsd:long .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#validUntil

hdc:validUntil rdf:type owl:DatatypeProperty ;
               
               rdfs:comment "Indicates how many milliseconds may have passed since January 1, 1970, 00:00:00 GMT for an individual to be still valid"@en ;
               
               rdfs:range xsd:long .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#VelocityInKmH

vo:VelocityInKmH rdf:type owl:DatatypeProperty ;
                 
                 rdfs:comment "Velocity in kilometer per hour" ;
                 
                 rdfs:subPropertyOf vo:VelocityType ;
                 
                 rdfs:range xsd:float .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#VelocityInMpS

vo:VelocityInMpS rdf:type owl:DatatypeProperty ;
                 
                 rdfs:comment "Velocity in meter per second" ;
                 
                 rdfs:subPropertyOf vo:VelocityType ;
                 
                 rdfs:range xsd:float .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#VelocityType

vo:VelocityType rdf:type owl:DatatypeProperty ;
                
                rdfs:domain vo:AbsoluteVelocity ;
                
                rdfs:range xsd:float .



###  http://www.opengis.net/ont/geosparql#asWKT

geo:asWKT rdf:type owl:DatatypeProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#AutoNomosNode

hdc:AutoNomosNode rdf:type owl:Class ;
                  
                  rdfs:subClassOf vo:Node .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#AutoNomosRoadsideUnit

hdc:AutoNomosRoadsideUnit rdf:type owl:Class ;
                          
                          rdfs:subClassOf vo:RoadsideUnit .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#Data

hdc:Data rdf:type owl:Class .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#DataCloud

hdc:DataCloud rdf:type owl:Class ;
              
              rdfs:subClassOf hdc:AutoNomosNode ,
                              hdc:Data ,
                              geo:Feature .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#DataUnit

hdc:DataUnit rdf:type owl:Class ;
             
             rdfs:subClassOf hdc:Data ,
                             geo:Feature .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#DestinationArea

hdc:DestinationArea rdf:type owl:Class ;
                    
                    rdfs:subClassOf vo:Area .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#DisseminationArea

hdc:DisseminationArea rdf:type owl:Class ;
                      
                      rdfs:subClassOf vo:Area .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#EquippedVehicle

hdc:EquippedVehicle rdf:type owl:Class ;
                    
                    rdfs:subClassOf hdc:MobileAutoNomosNode ,
                                    vo:Vehicle .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#HoveringDataCloud

hdc:HoveringDataCloud rdf:type owl:Class ;
                      
                      rdfs:subClassOf hdc:DataCloud ,
                                      hdc:MobileAutoNomosNode ;
                      
                      owl:disjointWith vo:QueryDrivenDataCloud .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#MobileAutoNomosNode

hdc:MobileAutoNomosNode rdf:type owl:Class ;
                        
                        rdfs:subClassOf hdc:AutoNomosNode ,
                                        vo:MobileNode ;
                        
                        rdfs:comment "A node is an object which is capable of producing and processing HDCs." .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#ProductionArea

hdc:ProductionArea rdf:type owl:Class ;
                   
                   rdfs:subClassOf vo:Area .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#QueryDrivenHoveringDataCloud

hdc:QueryDrivenHoveringDataCloud rdf:type owl:Class ;
                                 
                                 rdfs:subClassOf hdc:HoveringDataCloud .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#StationaryAutoNomosNode

hdc:StationaryAutoNomosNode rdf:type owl:Class ;
                            
                            rdfs:subClassOf hdc:AutoNomosNode ,
                                            hdc:StationaryNode .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#StationaryNode

hdc:StationaryNode rdf:type owl:Class ;
                   
                   rdfs:subClassOf vo:Node ;
                   
                   owl:disjointWith vo:MobileNode .



###  http://www.auto-nomos.de/ontologies/query-driven-data-clouds#TCrossing

hdc:TCrossing rdf:type owl:Class ;
              
              rdfs:subClassOf vo:Crossing .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#AbnormalLoad

vo:AbnormalLoad rdf:type owl:Class ;
                
                rdfs:subClassOf vo:Truck ;
                
                owl:disjointWith vo:HazardousMaterialsTransportation .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#AbsoluteVelocity

vo:AbsoluteVelocity rdf:type owl:Class ;
                    
                    rdfs:subClassOf vo:Velocity ;
                    
                    owl:disjointWith vo:RelativeVelocity .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#AcceleratingSensor

vo:AcceleratingSensor rdf:type owl:Class ;
                      
                      rdfs:subClassOf vo:Sensor .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#AccelerationManeuver

vo:AccelerationManeuver rdf:type owl:Class ;
                        
                        rdfs:subClassOf vo:TrafficManeuver .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Accident

vo:Accident rdf:type owl:Class ;
            
            rdfs:subClassOf vo:Hazard ,
                            vo:Obstacle .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#AmbulanceVehicle

vo:AmbulanceVehicle rdf:type owl:Class ;
                    
                    rdfs:subClassOf vo:EmergencyVehicle .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Animal

vo:Animal rdf:type owl:Class ;
          
          rdfs:subClassOf vo:TrafficParticipants .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Area

vo:Area rdf:type owl:Class ;
        
        rdfs:subClassOf vo:Location ,
                        sf:Polygon ;
        
        owl:disjointWith vo:Spot .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Backward

vo:Backward rdf:type owl:Class ;
            
            rdfs:subClassOf vo:Motion .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Bike

vo:Bike rdf:type owl:Class ;
        
        rdfs:subClassOf vo:TrafficParticipants .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#BreakingManeuver

vo:BreakingManeuver rdf:type owl:Class ;
                    
                    rdfs:subClassOf vo:DecelerationManeuver .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#BreakingSensor

vo:BreakingSensor rdf:type owl:Class ;
                  
                  rdfs:subClassOf vo:Sensor .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Bus

vo:Bus rdf:type owl:Class ;
       
       rdfs:subClassOf vo:Vehicle .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Compass

vo:Compass rdf:type owl:Class ;
           
           rdfs:subClassOf vo:Sensor .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#ConstructionWork

vo:ConstructionWork rdf:type owl:Class ;
                    
                    rdfs:subClassOf vo:Hazard ,
                                    vo:Obstacle .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#CountryRoad

vo:CountryRoad rdf:type owl:Class ;
               
               rdfs:subClassOf vo:RoadType .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Creation

vo:Creation rdf:type owl:Class ;
            
            rdfs:subClassOf vo:Instant .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Crest

vo:Crest rdf:type owl:Class ;
         
         rdfs:subClassOf vo:RoadSegment .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Crossing

vo:Crossing rdf:type owl:Class ;
            
            rdfs:subClassOf vo:RoadNode .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#DecelerationManeuver

vo:DecelerationManeuver rdf:type owl:Class ;
                        
                        rdfs:subClassOf vo:TrafficManeuver .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#DecreasingVelocity

vo:DecreasingVelocity rdf:type owl:Class ;
                      
                      rdfs:subClassOf vo:RelativeVelocity .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Destination

vo:Destination rdf:type owl:Class ;
               
               rdfs:subClassOf vo:Location ;
               
               rdfs:comment "The location, a data is sent to or a node is moving to"@en .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#DirectionSensor

vo:DirectionSensor rdf:type owl:Class ;
                   
                   rdfs:subClassOf vo:Sensor .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#EmergencyVehicle

vo:EmergencyVehicle rdf:type owl:Class ;
                    
                    rdfs:subClassOf vo:Vehicle .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#EstimatedTimeOfArrival

vo:EstimatedTimeOfArrival rdf:type owl:Class ;
                          
                          rdfs:subClassOf vo:Instant .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Expiration

vo:Expiration rdf:type owl:Class ;
              
              rdfs:subClassOf vo:Instant .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#FireEngine

vo:FireEngine rdf:type owl:Class ;
              
              rdfs:subClassOf vo:EmergencyVehicle .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Forward

vo:Forward rdf:type owl:Class ;
           
           rdfs:subClassOf vo:Motion .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#GeneralTrafficInformation

vo:GeneralTrafficInformation rdf:type owl:Class ;
                             
                             rdfs:subClassOf vo:Topic ;
                             
                             owl:disjointWith vo:Hazard .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Green

vo:Green rdf:type owl:Class ;
         
         rdfs:subClassOf vo:TrafficLightState .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Hazard

vo:Hazard rdf:type owl:Class ;
          
          rdfs:subClassOf vo:Topic .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#HazardousMaterialsTransportation

vo:HazardousMaterialsTransportation rdf:type owl:Class ;
                                    
                                    rdfs:subClassOf vo:Truck .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#HighVelocity

vo:HighVelocity rdf:type owl:Class ;
                
                rdfs:subClassOf vo:RelativeVelocity .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#IncreasingVelocity

vo:IncreasingVelocity rdf:type owl:Class ;
                      
                      rdfs:subClassOf vo:RelativeVelocity .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Infrastructure

vo:Infrastructure rdf:type owl:Class ;
                  
                  rdfs:subClassOf geo:Feature .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Instant

vo:Instant rdf:type owl:Class ;
           
           rdfs:subClassOf vo:Time ;
           
           owl:disjointWith vo:TimeInterval .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#InstantaneousVelocity

vo:InstantaneousVelocity rdf:type owl:Class ;
                         
                         rdfs:subClassOf vo:AbsoluteVelocity .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#JourneyTime

vo:JourneyTime rdf:type owl:Class ;
               
               rdfs:subClassOf vo:GeneralTrafficInformation ;
               
               owl:disjointWith vo:TrafficDensity .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Lane

vo:Lane rdf:type owl:Class ;
        
        rdfs:subClassOf vo:RoadSegment .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Lifetime

vo:Lifetime rdf:type owl:Class ;
            
            rdfs:subClassOf vo:TimeInterval .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Location

vo:Location rdf:type owl:Class ;
            
            owl:equivalentClass geo:Geometry ;
            
            rdfs:subClassOf geo:Geometry .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#LowVelocity

vo:LowVelocity rdf:type owl:Class ;
               
               rdfs:subClassOf vo:RelativeVelocity .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#MaximumVelocity

vo:MaximumVelocity rdf:type owl:Class ;
                   
                   rdfs:subClassOf vo:AbsoluteVelocity .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#MinimumVelocity

vo:MinimumVelocity rdf:type owl:Class ;
                   
                   rdfs:subClassOf vo:AbsoluteVelocity .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#MobileNode

vo:MobileNode rdf:type owl:Class ;
              
              rdfs:subClassOf vo:Node .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Motion

vo:Motion rdf:type owl:Class .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#MotorBike

vo:MotorBike rdf:type owl:Class ;
             
             rdfs:subClassOf vo:Vehicle .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Motorway

vo:Motorway rdf:type owl:Class ;
            
            rdfs:subClassOf vo:RoadType .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#MotorwayExit

vo:MotorwayExit rdf:type owl:Class ;
                
                rdfs:subClassOf vo:RoadSegment .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#NeighborhoodSensor

vo:NeighborhoodSensor rdf:type owl:Class ;
                      
                      rdfs:subClassOf vo:Sensor .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Node

vo:Node rdf:type owl:Class ;
        
        rdfs:subClassOf geo:Feature ;
        
        rdfs:comment "A node is an entity (actively) participating in VANET. Entities like unequipped vehicles, bikes, etc. which are not aware of the VANET around them, are not referred to as nodes."@en .



###  http://www.auto-nomos.de/ontologies/vanet-ontology#Obstacle

