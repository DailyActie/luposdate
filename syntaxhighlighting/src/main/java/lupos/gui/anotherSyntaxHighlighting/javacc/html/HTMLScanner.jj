/* -------------  HTML-Scanner -----------------*/options{  IGNORE_CASE = true;  JAVA_UNICODE_ESCAPE = true;  STATIC = false;}PARSER_BEGIN(HTMLScanner)package lupos.gui.anotherSyntaxHighlighting.javacc.html;public class HTMLScanner {}PARSER_END(HTMLScanner)SKIP :{  " "| "\t"| "\n"| "\r"| < "<!--" (~[])* "-->">
}TOKEN :{  < #PN_CHARS_BASE :    [ "A"-"Z" ]  | [ "a"-"z" ]  | [ "\u00C0"-"\u00D6" ]  | [ "\u00D8"-"\u00F6" ]  | [ "\u00F8"-"\u02FF" ]  | [ "\u0370"-"\u037D" ]  | [ "\u037F"-"\u1FFF" ]  | [ "\u200C"-"\u200D" ]  | [ "\u2070"-"\u218F" ]  | [ "\u2C00"-"\u2FEF" ]  | [ "\u3001"-"\uD7FF" ]  | [ "\uF900"-"\uFDCF" ]  | [ "\uFDF0"-"\uFFFD" ]  | [ "\u1000"-"\uEFFF" ] >| < #IDENTIFIER : (< PN_CHARS_BASE >) (< PN_CHARS_BASE > | ["0"-"9"])* >| < TEXT : (~[ "\"", "'", "<", ">",  "=" ])+ >| < TAG : ("<" < IDENTIFIER >) | (">") | ("/" ">") >| < ENDTAG : "<" "/" < IDENTIFIER > ">" >| < ATTRIBUTE : < IDENTIFIER > "=" >| < STRING : "\""    (      (~[ "\\", "\n", "\r", "\"" ])    | ("\\" ~[ "\n", "\r" ])    )*    "\""> }